{
    "name": "senku/lite",
    "description": "Lite is an acronym of Laravel + Inertia + Svelte web development stack. Currently, Laravel doesn't support Laravel Breeze with Inertia + Svelte, so I create my own.",
    "keywords": ["lite", "laravel", "inertia", "svelte"],
    "license": "MIT",
    "require": {
        "php": "^8.2.0",
        "illuminate/console": "^11.0",
        "illuminate/filesystem": "^11.0",
        "illuminate/support": "^11.0",
        "illuminate/validation": "^11.0",
        "symfony/console": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Senku\\Lite\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Senku\\Lite\\LiteServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
