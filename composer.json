{
    "name": "senkulabs/breeze-lite",
    "description": "Unofficial Laravel Breeze for Inertia + Svelte.",
    "keywords": ["laravel-breeze", "laravel", "inertia", "svelte"],
    "license": "MIT",
    "require": {
        "laravel/breeze": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "SenkuLabs\\Breeze\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SenkuLabs\\Breeze\\BreezeServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
